{
    "name": "rdrenth/tvrage-bundle",
    "description": "Symfony bundle for the adrenth/tvrage package",
    "keywords": ["tvrage", "tvrage.com", "api", "library", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/rdrenth/tvrage-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronald Drenth",
            "email": "ronalddrenth@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": ">=2.3,<3.0",
        "adrenth/tvrage": "^0.1",
        "doctrine/doctrine-cache-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/finder": "~2.3",
        "symfony/browser-kit": "~2.3"
    },
    "autoload": {
        "psr-0": {
            "Rdrenth\\TvrageBundle": ""
        }
    },
    "target-dir": "Rdrenth/TvrageBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
